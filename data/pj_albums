album.dark_matter <- data.frame(
  SongName = c("Scared of Fear", "React, Respond", "Wreckage", "Dark Matter", "Won't Tell", "Upper Hand", "Waiting for Stevie",
               "Running", "Something Special", "Got to Give", "Setting Sun")) |> 
  mutate(
    year = 2024,   
    album = 'Dark Matter',
    dm = 1)

